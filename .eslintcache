recatedRules":"57"},{"filePath":"58","messages":"59","errorCount":0,"warningCount":4,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\index.js",["60"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useState.js",[],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useInput.js",["61"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useTabs.js",["62"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useEffect.js",["63"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useTitle.js",["64"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useClick.js",["65","66","67","68"],"import React, { useState, useEffect, useRef } from 'react';\r\nimport ReactDOM from 'react-dom';\r\n/*\r\nuseRef 를 하는데 reference 란 뭘까?\r\nreference는 기본적으로 우리의 component의 어떤 부분을 \r\n선택할 수 있는 방법인데, document.getElementByID() 와 동등해.\r\n<input ref={potato} /> 이건,\r\n    input 에게 potato 를 참조하라고 하는 거야.\r\n\r\nconst App = () => {\r\n    const potato = useRef();\r\n    \r\n    setTimeout(() => console.log(potato), 3000);\r\n    return (\r\n        <div>\r\n            <div>Hi</div>\r\n            <input ref={potato} placeholder=\"swan\" />\r\n        </div>\r\n    );\r\n};\r\n    \r\n*/\r\nconst useClick = (onClick) => {\r\n    const element = useRef();\r\n    useEffect(() => {\r\n        if(element.current) {\r\n            element.current.addEventListener(\"click\", onClick);\r\n        }\r\n        return () => {\r\n            if(element.current) {\r\n                element.current.removeEventListener(\"click\", onClick);\r\n            }\r\n        };      \r\n    }, []);\r\n\r\n    if(typeof onClick !== \"function\") {\r\n        return;\r\n    }\r\n\r\n    return element;\r\n}\r\n/*dependency 가 [] 이면 무조건 mount 생성될 때 한번만. \r\nsayHello 를 가진 useClick 이 mount 되었을 때, click 이벤트를 추가할거야.\r\n이제는 뭔가 update 되었을때는 고려하지 않아도 되는 거지.\r\n*/\r\nconst App = () => {\r\n    const sayHello = () => console.log(\"say hello\");\r\n    const title = useClick(sayHello);\r\n    return (\r\n        <div>\r\n            <h1 ref={title}>Hi</h1>\r\n        </div>\r\n    );\r\n};\r\n/*\r\nreturn element;\r\n    reference를 return 해서 title 에 넣어주었고,\r\nuseEffect(() => { ~~~ });\r\n    에서 eventListener 를 추가해주었어.\r\n\r\n하! 지! 만! 여기서 중요한 것은 클린업 해줘야 한다는 거야.\r\ncomponentWillUnmount 될 때 eventListener를 지워야 한다는 거지.\r\nuseEffect 는 알다시피 componentDidmount 상태에 동작해.\r\n즉, component 가 mount 되었을 때 event 가 추가되는 거지.\r\n그 후에 componentWillUnMount 일때 event 발생 뒤 정리할 필요가 있어.\r\n그래서 지금부터 function 을 return 할꺼야.\r\n\r\n지금까지는 어떤 function 도 return 하지 않았지만, 이제는 할 필요가 있어.\r\nuseEffect를 return 받은 그 함수는 componentWillUnMount 때 호출될거야.\r\n*/\r\nexport default App;",["69","70"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useBeforeLeave.js",["71","72","73","74"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useFadeIn.js",["75","76","77"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useNetwork.js",["78","79","80"],"import React, { useState, useEffect, useRef } from 'react';\r\nimport ReactDOM from 'react-dom';\r\n\r\nconst useNetwork = (onChange) => {\r\n    const [status, setStatus] = useState(navigator.onLine);\r\n    const handleChange = () => {\r\n        if(typeof onChange === \"function\"){\r\n            onChange(navigator.onLine);\r\n        }\r\n        setStatus(navigator.onLine);\r\n    };\r\n    useEffect(() => {\r\n        window.addEventListener(\"online\", handleChange);\r\n        window.addEventListener(\"offline\", handleChange);\r\n        return () => {\r\n            window.removeEventListener(\"online\", handleChange);\r\n            window.removeEventListener(\"offline\", handleChange);\r\n        }\r\n    }, []);\r\n    return status;\r\n};\r\n\r\nconst App = () => {\r\n    const handleNetworkChange = (onoff) => {\r\n        console.log(onoff ? \"1\" : \"2\");\r\n    }\r\n    const onLine = useNetwork(handleNetworkChange);\r\n    return (\r\n        <div>\r\n            <h1>{onLine ? \"On\" : \"Off\"}</h1>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default App;",["81","82"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useScroll.js",["83","84"],"C:\\Users\\11\\Desktop\\JavaScript Practice\\JavaScript-Practice3\\react_hook_practice\\src\\App_useFullscreen.js",["85","86","87","88","89"],"import React, { useState, useEffect, useRef } from 'react';\r\nimport ReactDOM from 'react-dom';\r\nimport { isElementOfType } from 'react-dom/test-utils';\r\n\r\nconst useFullscreen = (callback) => {\r\n    const element = useRef();\r\n    const triggerFull = () => {\r\n        if (element.current) {\r\n            element.current.requestFullscreen();\r\n            if(callback && typeof callback === \"function\"){\r\n                callback(true);\r\n            }\r\n        }\r\n    };\r\n    const exitFull = () => {\r\n        document.exitFullscreen();\r\n        if(callback && typeof callback === \"function\"){\r\n            callback(false);\r\n        }\r\n    };\r\n    return {element, triggerFull, exitFull};\r\n};\r\nconst App = () => {\r\n    const onFulls = (isFull) => {\r\n        console.log(isFull ? \"We are full\" : \"not full\");\r\n    };\r\n    const {element, triggerFull, exitFull} = useFullscreen(onFulls);\r\n    return (\r\n        <div className=\"App\" style={{height: \"1000vh\"}}>\r\n            <div ref={element}>\r\n            <img \r\n                src=\"https://images.unsplash.com/photo-1592800919958-65678f3d80d3?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGV